{
    "manifest_version": 2,
    "name": "Cookie Monster",
    "version": "1.0",
    "description": "You get a cookie, you get a cookie, everyone gets a cookie!",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "cookies",
        "declarativeNetRequestWithHostAccess",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}